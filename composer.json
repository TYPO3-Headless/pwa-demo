{
    "repositories": [
      {
        "type": "composer",
        "url": "https://composer.typo3.org/"
      },
      {
        "type": "path",
        "url": "packages/*",
        "options": {
          "symlink": true
        }
      }
    ],
    "name": "friendsoftypo/pwademo",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Łukasz Uznański",
            "email": "extensions@macopedia.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "friendsoftypo3/headless": "4.x-dev",
        "helhum/typo3-console": "dev-main",
        "mask/mask": "^8.0",
        "pwademo/site-package": "^1",
        "typo3/cms-adminpanel": "^12.3",
        "typo3/cms-backend": "^12.3",
        "typo3/cms-belog": "^12.3",
        "typo3/cms-beuser": "^12.3",
        "typo3/cms-core": "^12.3",
        "typo3/cms-dashboard": "^12.3",
        "typo3/cms-extbase": "^12.3",
        "typo3/cms-extensionmanager": "^12.3",
        "typo3/cms-felogin": "^12.3",
        "typo3/cms-filelist": "^12.3",
        "typo3/cms-filemetadata": "^12.3",
        "typo3/cms-fluid": "^12.3",
        "typo3/cms-fluid-styled-content": "^12.3",
        "typo3/cms-form": "^12.3",
        "typo3/cms-frontend": "^12.3",
        "typo3/cms-impexp": "^12.3",
        "typo3/cms-indexed-search": "^12.3",
        "typo3/cms-info": "^12.3",
        "typo3/cms-install": "^12.3",
        "typo3/cms-linkvalidator": "^12.3",
        "typo3/cms-lowlevel": "^12.3",
        "typo3/cms-opendocs": "^12.3",
        "typo3/cms-reactions": "^12.3",
        "typo3/cms-recycler": "^12.3",
        "typo3/cms-redirects": "^12.3",
        "typo3/cms-reports": "^12.3",
        "typo3/cms-rte-ckeditor": "^12.3",
        "typo3/cms-scheduler": "^12.3",
        "typo3/cms-seo": "^12.3",
        "typo3/cms-setup": "^12.3",
        "typo3/cms-sys-note": "^12.3",
        "typo3/cms-t3editor": "^12.3",
        "typo3/cms-tstemplate": "^12.3",
        "typo3/cms-viewpage": "^12.3",
        "typo3/cms-workspaces": "^12.3",
        "typo3/minimal": "^12"
    },
    "config": {
      "sort-packages": true,
      "bin-dir": "bin",
      "vendor-dir": "vendor",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "platform": {
            "php": "8.1"
        }
    },
    "extra": {
      "typo3/minimal": {
        "web-dir": "public"
      }
    }
}
